{
    "languageserver": {
        "intelephense": {
            "command": "intelephense",
            "args": ["--stdio"],
            "filetypes": ["php"],
            "initializationOptions": {
                "storagePath": "/tmp/intelephense"
            }
        }
    },
    "snippets.extends": {
        "cpp": ["c"],
        "javascriptreact": ["javascript"],
        "typescript": ["javascript"]
    },
    "markdownlint.config": {
        "default": true,
        "line_length": false
    },
    "vetur.ignoreProjectWarning": true,
    "coc.source.emoji.triggerCharacters": [
        "TRIGGERCHAR"
    ]
}
